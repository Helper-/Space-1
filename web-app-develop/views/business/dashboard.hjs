                <div class="content">
                    <div class="row">
                      <table class="table table-striped">
                        <thead>
                          <tr>
                            <th>#</th>
                            <th>Name</th>
                            <th></th>
                            <th>Appointment Time</th>
                            <th>Status</th>
                            <th></th>
                            <th>Picture</th>
                            <th></th>
                          </tr>
                        </thead>

                        <tbody id="tblBody">
                          <tr>

                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>

                          </tr>
                        </tbody>
                      </table>
                    </div>
                </div>

            </section>

          </article>

